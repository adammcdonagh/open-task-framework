{
  "type": "transfer",
  "source": {
    "hostname": "{{ TEST_HOST }}",
    "directory": "{{ TEMP_SOURCE_FOLDER }}",
    "fileRegex": "test.*\\.log",
    "conditionals": {
      "size": {
        "gt": -1
      }
    },
    "fileWatch": {
      "timeout": 15,
      "directory": "/tmp",
      "fileRegex": "fileWatch.txt"
    },
    "logWatch": {
      "timeout": 15,
      "directory": "/tmp/testdir",
      "log": "log{{ YYYY }}Watch.log",
      "contentRegex": "someText"
    },
    "protocol": {
      "name": "ssh",
      "credentials": {
        "username": "adam",
        "password": "inzipped"
      }
    },
    "postCopyAction": {
      "action": "move",
      "destination": "/tmp/archive"
    }
  },
  "destination": {
    "hostname": "192.168.1.5",
    "directory": "/tmp/dest",
    "rename": {
      "pattern": "(.*).log",
      "sub": "\\1.txt"
    },
    "permissions": {
      "owner": "adam",
      "group": "dialout "
    },
    "mode": "0644",
    "transferType": "pull",
    "protocol": {
      "name": "ssh",
      "credentials": {
        "username": "adam",
        "password": "inzipped",
        "transferUsername": "adam"
      }
    }
  }
}