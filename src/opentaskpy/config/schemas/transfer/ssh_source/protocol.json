{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "http://localhost/transfer/ssh_source/protocol.json",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "enum": ["ssh"]
    },
    "port": {
      "type": "integer"
    },
    "credentials": {
      "type": "object",
      "properties": {
        "username": {
          "type": "string"
        },
        "keyFile": {
          "type": "string"
        },
        "key": {
          "type": "string"
        }
      },
      "required": ["username"],
      "additionalProperties": false
    }
  },
  "required": ["name", "credentials"],
  "additionalProperties": false
}
