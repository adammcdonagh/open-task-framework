{
  "type": "transfer",
  "source": {
    "hostname": "{{ HOST_A }}",
    "directory": "/tmp/testFiles/src",
    "fileRegex": ".*\\.log",
    "logWatch": {
      "timeout": 15,
      "log": "log{{ YYYY }}Watch.log",
      "contentRegex": "someText"
    },
    "protocol": {
      "name": "ssh",
      "credentials": {
        "username": "{{ SSH_USERNAME }}"
      }
    }
  }
}
